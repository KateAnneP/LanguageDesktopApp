<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.apka.FiszkaController">
    <children>
        <Pane prefHeight="400.0" prefWidth="156.0">
         <children>
            <Button layoutX="-1.0" layoutY="361.0" mnemonicParsing="false" onAction="#zamknijAction" prefHeight="39.0" prefWidth="156.0" text="Zamknij aplikację">
               <font>
                  <Font name="System Italic" size="14.0" />
               </font></Button>
            <Button fx:id="przycisk_zmien_zestaw" layoutX="-1.0" layoutY="14.0" mnemonicParsing="false" onAction="#zmienZestawAction" prefHeight="39.0" prefWidth="156.0" text="Zmień zestaw">
               <font>
                  <Font name="System Italic" size="18.0" />
               </font></Button>
            <Button fx:id="przycisk_zmien_jezyk" layoutY="73.0" mnemonicParsing="false" onAction="#zmienJezykAction" prefHeight="39.0" prefWidth="156.0" text="Zmień język">
               <font>
                  <Font name="System Italic" size="18.0" />
               </font></Button>
            <Button fx:id="przycisk_powrot" layoutY="130.0" mnemonicParsing="false" onAction="#powrotAction" prefHeight="39.0" prefWidth="156.0" text="Powrót">
               <font>
                  <Font name="System Italic" size="18.0" />
               </font></Button>
         </children>
        </Pane>
        <Pane layoutX="156.0" prefHeight="400.0" prefWidth="446.0">
         <children>
            <TextField fx:id="pole_fiszka" alignment="CENTER" editable="false" layoutX="19.0" layoutY="20.0" onMouseClicked="#zmienFiszkeAction" prefHeight="277.0" prefWidth="408.0" promptText="fiszka">
               <font>
                  <Font name="System Italic" size="36.0" />
               </font>
            </TextField>
            <Button fx:id="przycisk_wstecz" layoutX="172.0" layoutY="308.0" mnemonicParsing="false" onAction="#wsteczAction" text="&lt;--">
               <font>
                  <Font name="System Italic" size="14.0" />
               </font>
            </Button>
            <Button fx:id="przycisk_dalej" layoutX="238.0" layoutY="308.0" mnemonicParsing="false" onAction="#dalejAction" text="--&gt;">
               <font>
                  <Font name="System Italic" size="14.0" />
               </font>
            </Button>
         </children></Pane>
    </children>
</AnchorPane>
